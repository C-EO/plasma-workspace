#: config.qml:13
#, kde-format
msgid "General"
msgstr "ਆਮ"

#: ConfigGeneral.qml:36
#, kde-format
msgctxt ""
"Heading for a list of actions (leave, lock, switch user, hibernate, suspend)"
msgid "Show actions:"
msgstr "ਕਾਰਵਾਈਆਂ ਵੇਖਾਓ:"

#: ConfigGeneral.qml:37
#, kde-format
msgid "Log Out"
msgstr "ਲਾਗ ਆਉਟ"

#: ConfigGeneral.qml:44
#, kde-format
msgctxt "@option:check"
msgid "Show logout screen"
msgstr "ਲਾਗ-ਆਉਟ ਸਕਰੀਨ ਵੇਖਾਓ"

#: ConfigGeneral.qml:50
#, kde-format
msgid "Shut Down"
msgstr "ਬੰਦ ਕਰੋ"

#: ConfigGeneral.qml:56
#, kde-format
msgid "Restart"
msgstr "ਮੁੜ-ਸ਼ੁਰੂ ਕਰੋ"

#: ConfigGeneral.qml:62 data.js:5
#, kde-format
msgid "Lock"
msgstr "ਲਾਕ"

#: ConfigGeneral.qml:68
#, kde-format
msgid "Switch User"
msgstr "ਵਰਤੋਂਕਾਰ ਬਦਲੋ"

#: ConfigGeneral.qml:74 data.js:54
#, kde-format
msgid "Hibernate"
msgstr "ਹਾਈਬਰਨੇਟ"

#: ConfigGeneral.qml:80 data.js:47
#, kde-format
msgctxt "Suspend to RAM"
msgid "Sleep"
msgstr "ਸਲੀਪ"

#: data.js:6
#, kde-format
msgid "Lock the screen"
msgstr "ਸਕਰੀਨ ਨੂੰ ਲਾਕ ਕਰੋ"

#: data.js:12
#, kde-format
msgid "Switch user"
msgstr "ਵਰਤੋਂਕਾਰ ਬਦਲੋ"

#: data.js:13
#, kde-format
msgid "Start a parallel session as a different user"
msgstr "ਵੱਖਰੇ ਵਰਤੋਂਕਾਰ ਵਾਂਗ ਸਮਾਂਤਰ ਸ਼ੈਸ਼ਨ ਸ਼ੁਰੂ ਕਰੋ"

#: data.js:19
#, kde-format
msgid "Shutdown…"
msgstr "…ਬੰਦ ਕਰੋ"

#: data.js:20
#, kde-format
msgid "Turn off the computer"
msgstr "ਕੰਪਿਊਟਰ ਨੂੰ ਬੰਦ ਕਰੋ"

#: data.js:26
#, kde-format
msgid "Restart…"
msgstr "…ਮੁੜ-ਸ਼ੁਰੂ"

#: data.js:27
#, kde-format
msgid "Reboot the computer"
msgstr "ਕੰਪਿਊਟਰ ਨੂੰ ਮੁੜ ਚਾਲੂ ਕਰੋ"

#: data.js:33
#, kde-format
msgid "Logout…"
msgstr "…ਲਾਗਆਉਟ"

#: data.js:34
#, kde-format
msgid "End the session"
msgstr "ਸ਼ੈਸ਼ਨ ਖਤਮ ਕਰੋ"

#: data.js:40
#, kde-format
msgctxt "@info:tooltip"
msgid "Show logout screen"
msgstr "ਲਾਗ-ਆਉਟ ਸਕਰੀਨ ਵੇਖਾਓ"

#: data.js:41
#, kde-format
msgid "Log out, turn off, or restart the computer"
msgstr "ਕੰਪਿਊਟਰ ਨੂੰ ਲਾਗ ਆਉਟ, ਬੰਦ ਕਰੋ ਜਾਂ ਮੁੜ-ਚਾਲੂ ਕਰੋ"

#: data.js:48
#, kde-format
msgid "Sleep (suspend to RAM)"
msgstr "ਸਲੀਪ (RAM ਉੱਤੇ ਸਸਪੈਂਡ ਕਰੋ)"

