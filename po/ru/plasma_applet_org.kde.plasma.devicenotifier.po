#: actions/checkaction.cpp:74
#, kde-format
msgctxt "@action:button check a storageAccess (i.e disk) for error"
msgid "Check for Errors"
msgstr "Проверить на наличие ошибок"

#: actions/mountaction.cpp:103
#, kde-format
msgctxt "@action:button Mount a disk without verifying for errors"
msgid "Mount without verifying"
msgstr "Подключить без проверки"

#: actions/mountaction.cpp:106
#, kde-format
msgid "Mount"
msgstr "Подключить"

#: actions/mountandopenaction.cpp:179
#, kde-format
msgid "Open in File Manager"
msgstr "Открыть в диспетчере файлов"

#: actions/mountandopenaction.cpp:182
#, kde-format
msgid "Try to Fix"
msgstr "Попытаться устранить проблему"

#: actions/mountandopenaction.cpp:182
#, kde-format
msgid "Mount and Open"
msgstr "Подключить и открыть"

#: actions/mountandopenaction.cpp:184
#, kde-format
msgid "Eject"
msgstr "Извлечь"

#: actions/mountandopenaction.cpp:186
#, kde-format
msgid "Safely remove"
msgstr "Безопасно извлечь"

#: devicemessagemonitor_p.cpp:164
#, kde-format
msgid "This device has file system errors."
msgstr "Файловая система этого устройства содержит ошибки."

#: devicemessagemonitor_p.cpp:166
#, kde-format
msgctxt "@label device is a storage disk"
msgid "This device has no errors"
msgstr "Ошибок устройства не обнаружено"

#: devicemessagemonitor_p.cpp:169
#, kde-format
msgid "Successfully repaired!"
msgstr "Проблема устранена!"

#: devicemessagemonitor_p.cpp:173
#, kde-format
msgid "Device Status"
msgstr "Состояние устройства"

#: devicemessagemonitor_p.cpp:174
#, kde-format
msgid "A device can now be safely removed"
msgstr "Устройство может быть безопасно извлечено"

#: devicemessagemonitor_p.cpp:178
#, kde-format
msgid "This device can now be safely removed."
msgstr "Устройство может быть безопасно извлечено."

#: devicemessagemonitor_p.cpp:185
#, kde-format
msgid "You are not authorized to mount this device."
msgstr "У вас нет прав на подключение этого устройства."

#: devicemessagemonitor_p.cpp:189
#, kde-format
msgid "You are not authorized to eject this disc."
msgstr "У вас нет прав на извлечение диска из этого устройства."

#: devicemessagemonitor_p.cpp:192
#, kde-format
msgctxt "Remove is less technical for unmount"
msgid "You are not authorized to remove this device."
msgstr "У вас нет прав на отключение этого устройства."

#: devicemessagemonitor_p.cpp:195
#, kde-format
msgid "You are not authorized to repair this device."
msgstr "У вас нет прав на устранение проблемы с работой этого устройства."

