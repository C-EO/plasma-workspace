msgstr "Esperar inatividade antes de alternar:"

#: ui/DayNightLookAndFeel.qml:110
#, kde-format
msgctxt "@item:valuesuffix idle interval"
msgid "%1 second"
msgid_plural "%1 seconds"
msgstr[0] "%1 segundo"
msgstr[1] "%1 segundos"

#: ui/DayNightLookAndFeel.qml:122
#, kde-format
msgctxt "@info:tooltip"
msgid "Minimizes interruptions when actively using the computer."
msgstr "Minimiza interrupções enquanto o computador é usado de forma ativa."

